<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta title="OpenflPlayTest" package="com.example.openflplaytest" version="1.0.0" company="Dream Show Adventures"/>
	<app main="Main" path="Export" file="OpenflPlayTest" />
	
	<window width="240" height="430" fps="60" background="#FFFFFF" hardware="true" vsync="true" />
	
	<window if="web" orientation="portrait"  />
	<window if="desktop" orientation="portrait" fullscreen="false" resizable="true" />
	<window if="mobile" orientation="portrait" fullscreen="true" width="0" height="0" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	<haxelib name="openfl-play"/>
	
	<assets path="Assets" rename="assets" exclude="openfl.svg" />
	<icon path="Assets/openfl.svg" />
</project>